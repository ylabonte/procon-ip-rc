{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "appData": {
    "version": "0.0.1",
    "changelog": "development in progress..."
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.json",
          "/*.css",
          "/*.js",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "procon-ip",
      "urls": [
        "/GetState.csv",
        "**/GetState.csv"
      ],
      "cacheConfig": {
        "maxSize": 0,
        "maxAge": "0u",
        "strategy": "freshness"
      }
    }
  ]
}
